{
    "name": "arks",
    "version": "0.1.0",
    "description": "An opiniated React framework for isomorphic apps built on top of React with React Router and Apollo GraphQL",
    "readme": "<rootDir>/README.md",
    "homepage": "https://github.com/SugarDarius/arks#readme",
    "bugs": {
        "url": "https://github.com/SugarDarius/arks/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/SugarDarius/arks.git"
    },
    "license": "MIT",
    "author": {
        "name": "AurÃ©lien Dupays Dexemple",
        "url": "https://github.com/SugarDarius"
    },
    "keywords": [
        "React",
        "React Router",
        "Apollo GraphQL",
        "Isomorphic",
        "Express",
        "NodeJS",
        "TypeScript"
    ],
    "engines": {
        "node": ">=12.16.3",
        "npm": ">=6.13.0",
        "yarn": "yarn is not supported - please use npm"
    },
    "scripts": {
        "develop": "npm-run-all --serial develop:packages develop:cli",
        "develop:packages": "shx rm -rf node_modules/@arks && gulp packages:build:dev --dist node_modules/@arks",
        "develop:cli": "lerna run develop:cli --stream",
        "release": "npm-run-all --serial release:build:packages release:prepublish:packages release:publish:packages release:postpublish:packages",
        "release:build:packages": "gulp packages:build:prod",
        "release:prepublish:packages": "gulp packages:copy:misc:files:prod",
        "release:publish:packages": "lerna publish --force-publish --exact",
        "release:postpublish:packages": "gulp packages:clean:prod",
        "format": "prettier \"**/*.ts\" --ignore-path ./.prettierignore --write && git status",
        "lint:packages": "eslint \"packages/**/*.ts\" --fix --ignore-pattern \"packages/**/*.spec.ts\"",
        "reinstall": "node ./scripts/reinstall.script.js"
    },
